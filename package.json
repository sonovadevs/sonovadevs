{
  "name": "AudioNova",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:updatesnapshots": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "update-packages": "npm-check --update --save-exact",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./ --fix",
    "ts:check": "tsc --noEmit",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "storybook-android": "start-storybook -h 0.0.0.0",
    "build-storybook": "build-storybook",
    "postinstall": "patch-package",
    "prettier:write": "npx prettier --write **/*.{ts,tsx}",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "build:android": "cd android && ./gradlew bundleRelease"
  },
  "assets": [
    "./assets/fonts/icomoon"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/"
      ],
      "pattern": "**/?(*.)+(stories|story).[jt]s?(x)",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  },
